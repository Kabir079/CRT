int main()
{
    int number = 0;
    printf("Enter a number : ");
    scanf("%d",&number);
    
    swtich(number){
        case 10:
        printf("The number is equal to 10");
        break;
        case 50:
        printf("The number is equal to 50");
        break;
        case 100:
        printf("The number is equal to 100");
        break;
        default:
        printf("The number is not equal to 10,50 or 100");
        
    }
    return 0;
}
