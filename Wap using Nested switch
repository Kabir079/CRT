int main()
{
    int i = 10, j = 20;
    
    switch(i){
        case 10:
        printf("The valuse of i is evaluated in other switch : %d\n",i);
        case 20:
        switch(j){
            case 20:
            printf("The valuse of j is evaluated in other switch : %d\n",j);
    
        }
    }
    
    printf("Exact value of i is : %d\n",i);")
    printf("Exact value of j is : %d\n",j);")
}
