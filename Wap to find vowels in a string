#include <stdio.h>

void transpose(int p[3][3], int t[3][3]);

int main() 
{
    char ch[16] = "bharat";
    int i = 0;
    int count = 0;
    while(i<16)
    {
        if(ch[i] == 'a' || ch[i] == 'e' || ch[i] == 'i' || ch[i] == 'i' || ch[i] == 'u')
        {
            count++;
    
        }
        i++;
        
    }
    
    printf("The number of vowels is %d",count);
    
    return 0;
    
}
